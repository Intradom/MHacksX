            <string>// Set acceleration conditions
if (place_meeting(x, y + 1, obj_platform)) // Platform below
{
    y_accel = 0;
    y_vel = 0;
}
else
    y_accel = g;   

// Adjust velocities
x_vel += x_accel;
y_vel += y_accel;
if (x_vel &gt; hor_speed_max)
    x_vel = hor_speed_max;
else if (x_vel &lt; -hor_speed_max)
    x_vel = -hor_speed_max;
if (y_vel &lt; -fall_speed_max)
    y_vel = -fall_speed_max;

// Adjust positions
x += x_vel;
y += y_vel;
// Move back if velocity sent too far forward
while (place_meeting(x, y, obj_platform) &amp;&amp; place_meeting(x, y + 1, obj_platform))
    y--;
while (place_meeting(x, y, obj_platform) &amp;&amp; place_meeting(x, y - 1, obj_platform))
    y++;
while (place_meeting(x, y, obj_platform) &amp;&amp; place_meeting(x + 1, y, obj_platform))
    x--;
while (place_meeting(x, y, obj_platform) &amp;&amp; place_meeting(x - 1, y, obj_platform))
    x++;
